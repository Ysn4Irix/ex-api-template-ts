{
	"name": "ex-api-template-ts",
	"version": "0.0.1",
	"description": "An express typescript api starter template with dynamic routes, Powered by express",
	"main": "server.ts",
	"scripts": {
		"start": "node build/server.js",
		"dev": "ts-node-dev --respawn --transpile-only server.ts",
		"prebuild": "rimraf build",
		"build": "tsc && copyfiles ./public/* ./build",
		"format": "prettier --write \"./**/*.{ts,js,json}\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ysn4irix/ex-api-template-ts.git"
	},
	"keywords": [
		"api",
		"express",
		"nodejs",
		"typescript",
		"express-api",
		"express-api-template"
	],
	"author": {
		"name": "Ysn4Irix",
		"email": "ysn4irix@gmail.com",
		"url": "https://ysnirix.xyz"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ysn4irix/ex-api-template/issues"
	},
	"homepage": "https://github.com/ysn4irix/ex-api-template#readme",
	"dependencies": {
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-file-routing": "^3.0.3",
		"express-rate-limit": "^6.7.1",
		"express-slow-down": "^1.6.0",
		"helmet": "^7.0.0",
		"http-status-codes": "^2.2.0",
		"response-time": "^2.3.2",
		"serve-favicon": "^2.5.0",
		"signale": "^1.4.0"
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.16",
		"@types/express": "^4.17.21",
		"@types/express-slow-down": "^1.3.5",
		"@types/node": "^20.9.0",
		"@types/response-time": "^2.3.8",
		"@types/serve-favicon": "^2.5.7",
		"@types/signale": "^1.4.7",
		"copyfiles": "^2.4.1",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.0",
		"rimraf": "^5.0.5",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.2.2"
	}
}
